#!/bin/bash
echo "Compiling..."
g++ -O2 -o heuristic.exe main.cc
echo "Compilation done! Executing heurstics..."
sleep 1
N=8
for i in $(seq $N)
do
    echo "Our solution to problem $i:"
    ./heuristic.exe ../ilp/project.$i.dat gre
    echo "========================"
    echo "Real solution to problem $i:"
    cat ../ilp/project.$i.sol
    echo "========================"
done
rm heuristic.exe
